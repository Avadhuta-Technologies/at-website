---
import { getCollection } from 'astro:content';
import { generateCaseStudiesUrl, getAvailableIndustries } from '../../utils/industryUtils.js';

// Filter blog entries with 'draft: false' & date before current date
const publishedBlogEntries = await getCollection('blog', ({ data }) => {
  return !data.draft && data.publishDate < new Date();
});

// Sort content entries by publication date
publishedBlogEntries.sort(function (a, b) {
  return b.data.publishDate.valueOf() - a.data.publishDate.valueOf();
});

const footerLinks = [
  {
    header: 'Company',
    links: [
      {
        name: 'Home',
        href: '/',
        isExternal: false,
      },
      {
        name: 'Contact',
        href: '/contact',
        isExternal: false,
      },
    ],
  },
  {
    header: 'Services',
    links: [
      {
        name: 'Our Work',
        href: '/our-work',
        isExternal: false,
      },
      {
        name: 'Explore Pods',
        href: '/explore-pods',
        isExternal: false,
      },
      {
        name: 'Explore Packs',
        href: '/explore-packs',
        isExternal: false,
      },
      {
        name: 'Case Studies',
        href: '/case-studies',
        isExternal: false,
      },
    ],
  },
  {
    header: 'Resources',
    links: [     
      {
        name: 'Privacy Policy',
        href: '/assets/Privacy Policy.pdf',
        target: '_blank'
      },
      {
        name: 'Cookie Policy',
        href: '/assets/Cookie Policy.pdf',
        target: '_blank'
      },
      {
        name: 'Terms of Service',
        href: '/assets/Terms & Conditions.pdf',
        target: '_blank'
      },
    ],
  },
  {
    header: 'Case Studies',
    links: [
      {
        name: 'Ecommerce',
        href: generateCaseStudiesUrl('Ecommerce'),
        isExternal: false,
      },
      {
        name: 'Pharmacovigilance',
        href: generateCaseStudiesUrl('Pharmacovigilance'),
        isExternal: false,
      },
      {
        name: 'Mobile Apps / EdTech / Parenting',
        href: generateCaseStudiesUrl('Mobile Apps / EdTech / Parenting'),
        isExternal: false,
      },
      {
        name: 'Interior Design',
        href: generateCaseStudiesUrl('Interior Design'),
        isExternal: false,
      }
    ],
  },
];
